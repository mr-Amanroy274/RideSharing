{
  "expo": {
    "name": "RideSharing",
    "slug": "RideSharing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/car.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/car.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aashish_mahato.RideSharing"
    }
  }
}
